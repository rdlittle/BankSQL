            <Label disable="true" layoutX="268.0" layoutY="51.0" text="Columns" />
            <Spinner fx:id="spinColumns" disable="true" layoutX="350.0" layoutY="46.0" prefHeight="26.0" prefWidth="64.0" />
            <Label disable="true" layoutX="268.0" layoutY="94.0" text="Separator" />
