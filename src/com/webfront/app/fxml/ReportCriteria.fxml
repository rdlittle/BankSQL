<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="188.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.webfront.controller.ReportCriteriaController">
   <children>
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="100.0" prefWidth="573.0" spacing="20.0">
         <children>
            <VBox prefHeight="100.0" prefWidth="175.0">
               <children>
                  <Label text="Start date" />
                  <DatePicker fx:id="dpStart" prefHeight="25.0" prefWidth="165.0" />
               </children>
            </VBox>
            <VBox prefHeight="100.0" prefWidth="175.0">
               <children>
                  <Label text="End date" />
                  <DatePicker fx:id="dpEnd" prefHeight="25.0" prefWidth="182.0" />
               </children>
            </VBox>
            <VBox prefHeight="100.0" prefWidth="175.0">
               <children>
                  <Label text="Account" />
                  <ComboBox fx:id="cbAccount" prefHeight="25.0" prefWidth="192.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="422.0" layoutY="129.0" prefHeight="45.0" prefWidth="165.0" spacing="20.0">
         <children>
            <Button fx:id="btnOk" onAction="#onOk" mnemonicParsing="false" prefHeight="26.0" prefWidth="70.0" text="OK" />
            <Button fx:id="btnCancel" onAction="#onCancel" mnemonicParsing="false" prefHeight="26.0" prefWidth="70.0" text="Cancel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
