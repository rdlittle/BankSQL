                              <Label layoutX="192.0" layoutY="311.0" text="St" />
                              <TextField fx:id="txtPostalCode" layoutX="287.0" layoutY="326.0" />
                              <Label layoutX="287.0" layoutY="311.0" text="Postal Code" />
